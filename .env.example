PUSHER_APP_SECRET=
PUSHER_APP_CLUSTER=mt1

MIX_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
MIX_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

DEPLOY_REPOSITORY=git@github.com:awesome/example.com.git
DEPLOY_HEALTH_CHECK=https://example.com
DEPLOY_SERVER="example@xxx.xxx.xxx.xxx"
#DEPLOY_COMPOSER_BINARY="php /bin/composer"
